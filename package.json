{
	"name": "nest-starter",
	"version": "0.6.9",
	"description": "nest starter repository by simylein",
	"author": "simylein",
	"private": true,
	"license": "simylein",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint \"src/**/*.ts\" --fix",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/src/main",
		"schema:drop": "sh ./scripts/schema-drop.sh",
		"schema:sync": "sh ./scripts/schema-sync.sh",
		"database:drop": "yarn schema:drop && yarn schema:sync",
		"database:setup": "yarn schema:drop && yarn schema:sync && yarn schema:seed",
		"migration:run": "sh ./scripts/migration-run.sh",
		"migration:generate": "sh ./scripts/migration-generate.sh"
	},
	"dependencies": {
		"@nestjs/common": "9.0.7",
		"@nestjs/core": "9.0.7",
		"@nestjs/platform-express": "9.0.7",
		"@nestjs/swagger": "6.0.4",
		"@nestjs/typeorm": "9.0.0",
		"class-transformer": "0.5.1",
		"class-validator": "0.13.2",
		"compression": "1.7.4",
		"helmet": "5.1.1",
		"joi": "17.6.0",
		"mysql2": "2.3.3",
		"rimraf": "3.0.2",
		"rxjs": "7.5.6",
		"swagger-ui-express": "4.5.0",
		"typeorm": "0.2.45"
	},
	"devDependencies": {
		"@nestjs/cli": "9.0.0",
		"@nestjs/schematics": "9.0.1",
		"@types/compression": "1.7.2",
		"@types/express": "4.17.13",
		"@types/node": "18.6.2",
		"@types/rimraf": "3.0.2",
		"@typescript-eslint/eslint-plugin": "5.31.0",
		"@typescript-eslint/parser": "5.31.0",
		"eslint": "8.20.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin": "1.0.1",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-unused-imports": "2.0.0",
		"prettier": "2.7.1",
		"reflect-metadata": "0.1.13",
		"ts-loader": "9.3.1",
		"ts-node": "10.9.1",
		"tsconfig-paths": "4.0.0",
		"typescript": "4.7.4"
	}
}
